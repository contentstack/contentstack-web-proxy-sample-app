{
  "name": "javascript-sample-app",
  "version": "1.0.0",
  "description": "This is an example marketing website made using javascript/nodejs & Contentstack ",
  "main": "app.js",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "start": "node app.js",
    "format": "prettier  '**/*.{js,json,html,css}' --write",
    "lint:fix": "eslint '**/*.js' --fix"
  },
  "author": "Contentstack",
  "license": "MIT",
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.19.2",
    "contentstack": "^3.10.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "nunjucks": "^3.2.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.0.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,json,html,css}": [
      "npm run format"
    ]
  }
}
